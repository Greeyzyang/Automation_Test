<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <!--边框线宽度  边框线颜色-->
            <stroke
                android:width="1dp"
                android:color="#D3D3D3" />

            <!--圆角度数-->
            <corners android:radius="3dp" />

            <!--背景颜色-->
            <solid android:color="#ffffff" />

            <!--边距-->
            <padding android:right="5dp" />


        </shape>


    </item>
    <!--//第二组item 插入图片(替换默认箭头)-->
    <item>
        <bitmap
            android:gravity="end"
            />
    </item>
</layer-list>